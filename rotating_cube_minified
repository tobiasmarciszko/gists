c=document.getElementById("c").getContext("2d");a=2*Math.PI/360;ca=Math.cos(a);sa=Math.sin(a);cube={a:{x:-1,y:-1,z:4},c:{x:1,y:-1,z:4},g:{x:-1,y:1,z:4},d:{x:1,y:1,z:4},b:{x:-1,y:-1,z:6},f:{x:1,y:-1,z:6},h:{x:-1,y:1,z:6},i:{x:1,y:1,z:6}};t={};p=[];
function e(){c.clearRect(0,0,320,240);Object.keys(cube).forEach(function(d){var b=cube[d];z=b.z-5;y=b.y;x=b.x;b.y=y*ca-z*sa;b.z=y*sa+z*ca+5;z=b.z-5;b.x=x*ca+z*sa;b.z=-x*sa+z*ca+5;b=cube[d];t[d]={x:b.x/b.z*320+160,y:b.y/b.z*320+120}});p=[[t.a,t.c],[t.b,t.f],[t.a,t.g],[t.c,t.d],[t.g,t.d],[t.c,t.f],[t.d,t.i],[t.i,t.h],[t.h,t.b],[t.i,t.f],[t.g,t.h],[t.a,t.b]];p.forEach(function(d){c.beginPath();c.moveTo(d[0].x,d[0].y);c.lineTo(d[1].x,d[1].y);c.closePath();c.stroke()});requestAnimationFrame(e)}e();
